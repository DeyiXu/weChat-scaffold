<tab1 wx:if="{{tabsIndex=='tab1'}}"></tab1>
<tab2 wx:if="{{tabsIndex=='tab2'}}"></tab2>
<tab3 wx:if="{{tabsIndex=='tab3'}}"></tab3>
<tab4 wx:if="{{tabsIndex=='tab4'}}"></tab4>

<view class="cu-bar tabbar bg-white shadow foot">
  <view class="action" bindtap="NavChange" data-cur="tab1">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/tab1{{tabsIndex=='tab1'?'_cur':''}}.png"></image>
    </view>
    <view class="{{tabsIndex=='tab1'?'text-green':'text-gray'}}">元素</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="tab2">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/tab2{{tabsIndex=='tab2'?'_cur':''}}.png"></image>
    </view>
    <view class="{{tabsIndex=='tab2'?'text-green':'text-gray'}}">组件</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="tab3">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/tab3{{tabsIndex=='tab3'?'_cur':''}}.png"></image>
    </view>
    <view class="{{tabsIndex=='tab3'?'text-green':'text-gray'}}">扩展</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="tab4">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/tab4{{tabsIndex=='tab4'?'_cur':''}}.png"></image>
    </view>
    <view class="{{tabsIndex=='tab4'?'text-green':'text-gray'}}">关于</view>
  </view>
</view>